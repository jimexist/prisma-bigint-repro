generator client {
  provider        = "prisma-client"
  previewFeatures = ["queryCompiler", "driverAdapters"]
  relationMode    = "prisma"
  output          = "../src/generated/prisma"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
}

model User {
  id BigInt @id @default(autoincrement())
  name String
}
